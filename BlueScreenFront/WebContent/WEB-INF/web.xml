<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5"> 
<welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
<servlet>
  <servlet-name>Administrador</servlet-name>
  <servlet-class>br.com.fiap.controller.Administrador</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>Administrador</servlet-name>
  <url-pattern>/servlet/</url-pattern>
</servlet-mapping>
<servlet>
  <servlet-name>LoginServlet</servlet-name>
  <servlet-class>br.com.fiap.controller.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/login/</url-pattern>
</servlet-mapping>
<servlet>
  <servlet-name>Carrinho</servlet-name>
  <servlet-class>br.com.fiap.controller.Carrinho</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>Carrinho</servlet-name>
  <url-pattern>/carrinho/</url-pattern>
</servlet-mapping>
</web-app>
